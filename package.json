{
  "name": "closure-webpack-sample",
  "description": "Sample for closure-webpack-plugin",
  "version": "1.0.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=8"
  },
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:mkdirp build:deps build:webpack",
    "build:deps": "./node_modules/google-closure-library/closure/bin/build/depswriter.py --root_with_prefix 'src ../../../../src' > dist/deps.js",
    "build:mkdirp": "mkdirp dist",
    "build:webpack": "webpack",
    "start": "npx st . -nc",
    "test": "run-s test:lint",
    "test:lint": "eslint src *.js"
  },
  "dependencies": {},
  "devDependencies": {
    "closure-webpack-plugin": "github:teppeis/closure-webpack-plugin#fix",
    "eslint": "^4.14.0",
    "eslint-config-teppeis": "^6.3.1",
    "google-closure-library": "^20171203.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.10.0"
  },
  "homepage": "https://github.com/teppeis/closure-webpack-sample",
  "repository": "teppeis/closure-webpack-sample",
  "bugs": "https://github.com/teppeis/closure-webpack-sample/issues",
  "keywords": [
    "closure-compiler",
    "google-closure-compiler",
    "webpack"
  ],
  "license": "MIT"
}
